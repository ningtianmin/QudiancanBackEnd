-- ----------------------------
DROP TABLE IF EXISTS `authority`;
CREATE TABLE `authority` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `path` varchar(64) NOT NULL COMMENT '访问路径',
  `name` varchar(16) DEFAULT NULL COMMENT '名称',
  `role_ids` varchar(255) DEFAULT NULL COMMENT '所属角色',
  `description` varchar(64) DEFAULT NULL COMMENT '描述',
  PRIMARY KEY (`id`),
  UNIQUE KEY `authoritycol_UNIQUE` (`path`)
) ENGINE=InnoDB AUTO_INCREMENT=27 DEFAULT CHARSET=utf8 COMMENT='权限';

