-- ----------------------------
-- Table structure for branch_order
-- ----------------------------
DROP TABLE IF EXISTS `branch_order`;
CREATE TABLE `branch_order` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `branch_id` int(10) unsigned NOT NULL COMMENT '门店id',
  `branch_name` varchar(16) NOT NULL COMMENT '门店名称',
  `table_id` int(10) unsigned DEFAULT NULL COMMENT '桌台id',
  `member_id` int(10) unsigned DEFAULT NULL COMMENT '会员id',
  `order_number` varchar(32) NOT NULL COMMENT '订单编号',
  `total_sum` decimal(8,2) NOT NULL COMMENT '订单金额',
  `discount_sum` decimal(8,2) DEFAULT NULL COMMENT '折扣金额',
  `wipe_sum` decimal(4,2) DEFAULT NULL COMMENT '抹零金额',
  `charge_sum` decimal(8,2) DEFAULT NULL COMMENT '实收金额',
  `table_name` varchar(16) DEFAULT NULL COMMENT '桌台名称',
  `note` varchar(255) DEFAULT NULL COMMENT '订单备注',
  `pay_method` varchar(16) DEFAULT NULL COMMENT '支付方式',
  `pay_status` varchar(16) NOT NULL COMMENT '支付状态',
  `order_status` varchar(16) NOT NULL COMMENT '订单状态',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `customer_num` int(11) NOT NULL COMMENT '客人数',
  PRIMARY KEY (`id`),
  KEY `fk_branch_order_subject_shop_branch1` (`branch_id`),
  KEY `fk_branch_order_member1` (`member_id`),
  KEY `fk_branch_order_branch_table1` (`table_id`),
  CONSTRAINT `fk_branch_order_branch_table1` FOREIGN KEY (`table_id`) REFERENCES `branch_table` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_order_member1` FOREIGN KEY (`member_id`) REFERENCES `member` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_order_subject_shop_branch1` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='门店订单';

-- ----------------------------
-- Records of branch_order
-- ----------------------------

-- ----------------------------
-- Table structure for branch_product
-- ----------------------------
DROP TABLE IF EXISTS `branch_product`;
CREATE TABLE `branch_product` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `branch_id` int(10) unsigned NOT NULL COMMENT '门店id',
  `category_id` int(10) unsigned NOT NULL COMMENT '产品类目id',
  `department_id` int(10) unsigned NOT NULL COMMENT '出品部门id',
  `name` varchar(16) NOT NULL COMMENT '名称',
  `image` varchar(255) DEFAULT NULL COMMENT '图片',
  `unit_name` varchar(16) NOT NULL COMMENT '单位',
  `price` decimal(8,2) NOT NULL COMMENT '单价',
  `description` varchar(255) DEFAULT NULL COMMENT '描述',
  `position` int(10) unsigned NOT NULL COMMENT '产品类目内排序',
  `status` varchar(16) NOT NULL COMMENT '状态',
  PRIMARY KEY (`id`),
  KEY `fk_branch_product_shop_branch1` (`branch_id`),
  KEY `fk_branch_product_branch_product_category1` (`category_id`),
  KEY `fk_branch_product_branch_department1` (`department_id`),
  CONSTRAINT `fk_branch_product_branch_department1` FOREIGN KEY (`department_id`) REFERENCES `department` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_product_branch_product_category1` FOREIGN KEY (`category_id`) REFERENCES `product_category` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_product_shop_branch1` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='门店产品';

-- ----------------------------
-- Records of branch_product
-- ----------------------------

-- ----------------------------
-- Table structure for branch_table
-- ----------------------------
DROP TABLE IF EXISTS `branch_table`;
CREATE TABLE `branch_table` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `branch_id` int(10) unsigned NOT NULL COMMENT '门店id',
  `category_id` int(10) unsigned NOT NULL COMMENT '类目id',
  `order_id` int(10) unsigned DEFAULT NULL COMMENT '占用该桌台的订单id',
  `name` varchar(16) NOT NULL COMMENT '名称',
  `capacity` int(10) unsigned NOT NULL COMMENT '可供就餐人数',
  `position` int(10) unsigned NOT NULL COMMENT '桌台类目内排序',
  `status` varchar(16) NOT NULL COMMENT '状态',
  `qrcode` varchar(255) DEFAULT NULL COMMENT '小程序二维码地址',
  PRIMARY KEY (`id`),
  KEY `fk_branch_table_shop_branch1` (`branch_id`),
  KEY `fk_branch_table_branch_table_category1` (`category_id`),
  KEY `fk_branch_table_branch_order_subject1` (`order_id`),
  CONSTRAINT `fk_branch_table_branch_order_subject1` FOREIGN KEY (`order_id`) REFERENCES `branch_order` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_table_branch_table_category1` FOREIGN KEY (`category_id`) REFERENCES `table_category` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_branch_table_shop_branch1` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='门店桌台';

