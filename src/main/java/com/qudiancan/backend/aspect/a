DROP TABLE IF EXISTS `cart`;
CREATE TABLE `cart` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `branch_id` int(10) unsigned NOT NULL COMMENT '门店id',
  `member_id` int(11) NOT NULL COMMENT '微信用户会员id',
  PRIMARY KEY (`id`),
  KEY `fk_wechat_cart_subject_shop_branch1` (`branch_id`),
  CONSTRAINT `fk_wechat_cart_subject_shop_branch1` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='购物车';

