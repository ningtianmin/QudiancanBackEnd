-- ----------------------------
-- Table structure for branch
-- ----------------------------
DROP TABLE IF EXISTS `branch`;
CREATE TABLE `branch` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '主键',
  `shop_id` varchar(16) NOT NULL COMMENT '餐厅id',
  `name` varchar(16) NOT NULL COMMENT '名称',
  `notice` varchar(255) DEFAULT NULL COMMENT '公告',
  `phone` varchar(16) NOT NULL COMMENT '手机号',
  `address` varchar(255) NOT NULL COMMENT '地址',
  `longitude` varchar(16) NOT NULL COMMENT '经度',
  `latitude` varchar(16) NOT NULL COMMENT '纬度',
  `introduction` varchar(255) DEFAULT NULL COMMENT '介绍',
  `status` varchar(16) NOT NULL COMMENT '状态',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  PRIMARY KEY (`id`),
  KEY `fk_shop_branch_shop_subject1` (`shop_id`),
  CONSTRAINT `fk_shop_branch_shop_subject1` FOREIGN KEY (`shop_id`) REFERENCES `shop` (`id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 COMMENT='门店';

