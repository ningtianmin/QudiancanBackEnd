-- ----------------------------
-- Table structure for shop
-- ----------------------------
DROP TABLE IF EXISTS `shop`;
CREATE TABLE `shop` (
  `id` varchar(16) NOT NULL COMMENT '主键',
  `name` varchar(16) DEFAULT NULL COMMENT '餐厅或品牌名称',
  `holder_type` varchar(16) DEFAULT NULL COMMENT '主体类型',
  `holder_name` varchar(16) DEFAULT NULL COMMENT '个人名称或企业名称',
  `holder_identify` varchar(18) DEFAULT NULL COMMENT '个人身份证或企业统一社会信用代码',
  `telephone` varchar(16) DEFAULT NULL COMMENT '电话或手机',
  `introduction` varchar(255) DEFAULT NULL COMMENT '介绍',
  `status` varchar(16) NOT NULL COMMENT '状态',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='餐厅';

